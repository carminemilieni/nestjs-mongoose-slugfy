{
  "name": "nestjs-mongoose-slugfy",
  "version": "0.0.1",
  "description": "A helper for create unique slug with nestjs and mongoose",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/carminemilieni/nestjs-mongoose-slugfy",
  "author": "Carmine Milieni <carmine.milieni@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "start:dev": "tsc -p tsconfig.json -w",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
